{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content"
        ]
      }
    ],
    "css-prefix-order": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 107",
    "not firefox <= 108",
    "not ios_saf <= 14.8",
    "not opera <= 92",
    "not safari <= 14.1"
  ]
}